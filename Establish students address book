#include<stdio.h>
#define NUM 3
struct men
{
  char name[20];
  char phone[10];
};
void main()
{
  struct men man[NUM];
  int i;
  for(i=0;i<NUM;i++)
  {
   printf(“input name:\n”);
   gets(man[i].name);
   printf(“input phone:\n”);
   gets(man[i].phone);
  }
  printf(“name\t\t\tphone\n”);
  for(i=0;i<NUM;i++)
printf(“%s\t\t%s\n”,man[i].name,man[i].phone);
}
